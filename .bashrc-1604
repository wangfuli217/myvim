inorun() (
  # Run command whenever a given file or a file
  # in the given directory changes.
  # inorun <file> <cmd>...
  file="$1"
  shift
  cmd="$@"
  # Run the command once when we start.
  eval "$cmd"
  while true; do \
    eval "$cmd"
    inotifywait -qre close_write "$file"
  done
)

runn() {
  n="$1"
  shift
  i=0
  while [ "$i" -lt "$n" ]; do
      if ! $*; then
        echo "FAIL i = $i"
        break
      fi
      i="$(($i + 1))"
  done
}

